<div class="container">
    
    <div class="row">
    <div class="col-xs-10 col-xs-offset-1">
      <h4>注文履歴一覧</h4>

      <table class="table">
        <thead>
          <tr>
            <th>購入日時</th>
            <th>購入者</th>
            <th>注文個数</th>
            <th>注文ステータス</th>
          </tr>
        </thead>

        <tbody>
          <% @orders.each do |order| %>
            <tr>
              <td><%= order.updated_at %></td>
              <td><%= order.last_name %><%= customer.first_name %></td>
              <td><%= order.order_details.each order_details %></td>
             
             
                <%= paginate @orders %>
            </tr>
            <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>