<div class="col-xs-2">
    <table class="table table-bordered table-hover text-center ec-genre-list" >
      <tr>
        <td>
          <%= link_to "ジャンル検索", items_path  %>
        </td>
      </tr>

      <% genres.each do |genre| %>
        <tr>
          <td>
            <%= link_to genre.name, admin_item_path(genre.id) %>
            </td>
        </tr>
      <% end %>
    </table>
</div>